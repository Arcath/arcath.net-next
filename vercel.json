{
  "github": {
    "enabled": false
  },
  "redirects": [{"source": "/articles", "destination": "/posts"}],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Permissions-Policy",
          "value": "interest-cohort=()"
        }
      ]
    }
  ],
  "functions": {
    "pages/_data/posts/[skip].js": {
      "includeFiles": "_content/posts/**"
    }
  }
}
